{
    "manifest_version": 2,
    "name": "PhobiaBlocker",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "js/jquery-3.4.1.min.js",
            "js/compromise.min.js",
            "js/natural.js",
            "js/stopWords.js",
            "js/js.js"
        ],
        "css": [
            "css/style.css"
        ]
    }],
    "background": {
        "scripts": [
            "js/jquery-3.4.1.min.js",
            "js/background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs",
        "contextMenus",
        "storage",
        "cookies"
    ]
}